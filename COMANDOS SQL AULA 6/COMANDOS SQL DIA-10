select * from funcionario;

select dep_id, avg(fun_sal), sum(fun_sal) from funcionario 
group by dep_id having avg(fun_sal) > 3249.;

select fun_nome from funcionario where fun_sal in( select max(fun_sal) from funcionario);


select fun_nome from funcionario where fun_sal = ( select max(fun_sal) from funcionario);





