select * from editora;

select * from emprestimo;

select e.edi_descricao from editora e where edi_cod = 1;
select * from telefone;

select count(pes_nome) from pessoa p, telefone t where p.pes_cod = t.tel_cod and t.tel_ddd = 12;
select * from aluno;
select * from pessoa;
select * from professor;
select * from telefone;
select p.pes_nome from pessoa p 
	inner join aluno a on p.pes_cod = a.pes_cod
	inner join emprestimo e on p.pes_cod = e.emp_cod;
    
    
select p.pes_nome,pr.titulacao,t.tel_numero from pessoa p
	left outer join professor pr on p.pes_cod = pr.pes_cod
    left outer join telefone t on p.pes_cod = t.pes_cod;

select * from telefone;
SELECT p.pes_nome, a.pes_nro_matricula, t.tel_numero 
FROM Pessoa p 
LEFT OUTER JOIN Aluno a ON p.pes_cod = a.pes_cod
LEFT OUTER JOIN Telefone t ON p.pes_cod = t.pes_cod;

select p.pes_nome,a.pes_nro_matricula,t.tel_cod from pessoa p
left outer join aluno a on p.pes_cod = a.pes_cod
left outer join telefone t on p.pes_cod = t.pes_cod;




create table partido(
idPartido int(4) primary key,
siglaPartido int(4),
descricaoPartido varchar(30)
);
create table deputado(
idDeputado int(4) primary key,
idPartido int(4),
nomeDeputado varchar(30),
foreign key (idPartido) references partido(idPartido)
);

create table sessao(
idSessao int(4) primary key,
dataSessao date,
horaSessao varchar(4),
decisao varchar(4)
);
create table participacao(
idSessao int(4),
idDeputado int(4),
primary key(idSessao,idDeputado),
foreign key (idSessao) references sessao(idSessao),
foreign key (idDeputado) references deputado(idDeputado)
)
;

alter table participacao
add constraint idDeputado UNIQUE(idDeputado);

-- foreign key (idDeputado) references tabela(idDeputado);


-- primary key ( coluna1, coluna2);
-- foreign key ( coluna especificada) references nometabela(coluna especificada);
-- alter table especifica
-- add constraint nomedaconstraint tipoconstraint(nomedacolunaespecifica)
select * from deputado;


Alter table deputado
Add column data_cadastro DATE DEFAULT current_timestamp;



select d.idDeputado, d.idPartido, p.siglaPartido, s.decisao from deputado d, partido p, sessao s;









create table departamento
(dep_id int primary key,
 dep_descricao varchar(30));

create table funcionario
(fun_cod int primary key,
 fun_nome varchar(20),
 fun_sal decimal(10,2),
 dep_id int references departamento (dep_id));

create table telefone2
(tel_codigo int primary key,
 func_cod int references funcionario(func_cod),
 tel_numero int );

